
<div>
<h2> Show Book </h2>

<table>
    <tr>
        <th> ID </th>
        <td> <%= @book.id %></td>
    </tr>
    <tr>
        <th> Title </th>
        <td> <%= @book.title %></td>
    </tr>
    <tr>
        <th> Author </th>
        <td> <%= @book.author %></td>
    </tr>
    <tr>
        <th> Genre </th>
        <td> <%= @book.genre %></td>
    </tr>
    <tr>
        <th> Price </th>
        <td> <%= @book.price %></td>
    </tr>
    <tr>
        <th> Publish Date </th>
        <td> <%= @book.publishDate %></td>
    </tr>
</table>

</div>
<a href="/books/index">Back to Library</a> <br>

    